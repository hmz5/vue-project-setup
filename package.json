    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "fastclick": "^1.0.6",
    "jquery": "^3.1.0",
    "vue": "^2.4.2",
    "vue-resource": "^0.9.3",
    "vue-router": "^2.2.1",
    "vuex": "^2.1.2"
  }
