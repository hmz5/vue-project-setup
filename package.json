{
  "name": "vue-project-setup",
  "version": "1.0.0",
  "description": "关于vue搭建项目的一些整理",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development PORT=8080 supervisor -w build -i src build/server.js",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached --bail --optimize-minimize  --config ./build/webpack.prod.config.js",
    "start": "cross-env NODE_ENV=production PORT=5000 pm2 start build/ecosystem.config.js",
    "restart": "pm2 reload vue-project-setup",
    "stop": "pm2 stop vue-project-setup",
    "logApp": "pm2 logs vue-project-setup",
    "delete": "pm2 delete vue-project-setup"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "axios": "^0.17.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-plugin-vuefix": "^0.2.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "node-sass": "^4.5.3",
    "opn": "^5.1.0",
    "postcss-loader": "^0.11.1",
    "postcss-pxtorem": "^4.0.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "supervisor": "^0.12.0",
    "url-loader": "^0.5.8",
    "vue-loader": "^12.2.1",
